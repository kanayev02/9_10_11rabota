<Window x:Class="_91011rabota.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_91011rabota"
        mc:Ignorable="d"
        Title="Гимназия" Height="450" Width="800" Icon="Resources/image.ico">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="30"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <StackPanel Orientation="Horizontal">
            <TextBlock Text="Гимназия" FontSize="20"/>
            <Button Content="Вход" Height="20" Click="Button_Click"/>
            <Button Content="Регистрация" Height="20" Click="Button_Click_1"/>
        </StackPanel>
        <Frame x:Name="Frame" Grid.Row="1" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
